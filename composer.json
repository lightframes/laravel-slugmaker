{
    "name": "lightframes/laravel-slugmaker",
    "description": "Generate slugs when saving Eloquent models in a separate table",
    "keywords": [
        "laravel-slugmaker",
        "slug"
    ],
    "homepage": "https://github.com/lightframes/laravel-slugmaker",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefano Lebbolo",
            "email": "stefano.lebbolo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.4.33",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"

    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Lightframes\\SlugMaker\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lightframes\\SlugMaker\\SlugMakerServiceProvider"
            ]
        }
    }
}
